import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns

# Generate synthetic data
np.random.seed(0)  # For reproducibility
n = 100  # Number of samples

ages = np.random.randint(18, 70, size=n)
genders = np.random.choice(['Male', 'Female'], size=n)

# Create DataFrame
df = pd.DataFrame({
    'age': ages,
    'gender': genders
})

# Bar chart for gender distribution
plt.figure(figsize=(8, 6))
sns.countplot(x='gender', data=df)
plt.title('Gender Distribution')
plt.xlabel('Gender')
plt.ylabel('Count')
plt.show()

# Histogram for age distribution
plt.figure(figsize=(8, 6))
sns.histplot(df['age'], bins=10, kde=True)
plt.title('Age Distribution')
plt.xlabel('Age')
plt.ylabel('Frequency')
plt.show()

one more example :
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns

# Generate synthetic data
np.random.seed(0)  # For reproducibility
n = 100  # Number of samples

ages = np.random.randint(18, 70, size=n)  # Generate random ages between 18 and 70
genders = np.random.choice(['Male', 'Female'], size=n)  # Randomly assign genders

# Create DataFrame
df = pd.DataFrame({
    'age': ages,
    'gender': genders
})

# Bar chart for gender distribution
plt.figure(figsize=(8, 6))
sns.countplot(x='gender', data=df)
plt.title('Gender Distribution')
plt.xlabel('Gender')
plt.ylabel('Count')
plt.show()

# Histogram for age distribution
plt.figure(figsize=(8, 6))
sns.histplot(df['age'], bins=10, kde=True)
plt.title('Age Distribution')
plt.xlabel('Age')
plt.ylabel('Frequency')
plt.show()

